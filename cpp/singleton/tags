!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.5.4	//
CC	makefile	/^CC = clang++$/;"	m
CFLAGS	makefile	/^CFLAGS = -std=c++11$/;"	m
INSTANCE	Singleton.cpp	/^Singleton* Singleton::INSTANCE = nullptr;$/;"	m	class:Singleton	file:
INSTANCE	Singleton.h	/^  static Singleton *INSTANCE;$/;"	m	class:Singleton
Singleton	Singleton.cpp	/^Singleton::Singleton()$/;"	f	class:Singleton
Singleton	Singleton.h	/^class Singleton$/;"	c
getInstance	Singleton.cpp	/^static void Singleton::getInstance()$/;"	f	class:Singleton
main	main.cpp	/^int main()$/;"	f
singletonOperation	Singleton.cpp	/^void Singleton::singletonOperation()$/;"	f	class:Singleton
~Singleton	Singleton.cpp	/^Singleton::~Singleton()$/;"	f	class:Singleton
